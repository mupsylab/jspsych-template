{
  "name": "jspsych-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@jspsych/plugin-call-function": "^1.1.2",
    "@jspsych/plugin-fullscreen": "^1.1.2",
    "@jspsych/plugin-html-button-response": "^1.1.2",
    "@jspsych/plugin-html-keyboard-response": "^1.1.1",
    "@jspsych/plugin-instructions": "^1.1.2",
    "@jspsych/plugin-preload": "^1.1.2",
    "@jspsych/plugin-survey-html-form": "^1.0.2",
    "core-js": "^3.8.3",
    "jquery": "^3.6.3",
    "jspsych": "^7.3.0",
    "vue": "^3.2.13"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {
      "strict": "off",
      "vue/no-unused-components": "off",
      "generator-star-spacing": "off",
      "no-debugger": "off",
      "no-tabs": "off",
      "no-unused-vars": "off",
      "no-console": "off",
      "no-irregular-whitespace": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "vue/no-unused-components": [
    "error",
    {
      "ignoreWhenBindingPresent": true
    }
  ]
}
